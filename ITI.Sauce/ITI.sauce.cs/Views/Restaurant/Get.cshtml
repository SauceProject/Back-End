
@using ITI.Sauce.ViewModels



@model PaginingViewModel<List<RestaurantViewModel>>


@section StyleSection{
    <link rel="stylesheet" href="~/Content/CSS/Restaurant.css" />
}



<h2>Restaurant</h2>

<table>
  <tr>
    <th>Name</th>
    <th>IsDeleted</th>
    <th>RegisterDate</th>
    <th>NameAr</th>

  </tr>
	@foreach (var U in Model.Data)
{  
  <tr>
	<td>@U.NameEN</td>
	<td>@U.IsDeleted</td>
	<td>@U.RegisterDate</td>
	<td>@U.NameAR</td>

  </tr>

    }


</table>

