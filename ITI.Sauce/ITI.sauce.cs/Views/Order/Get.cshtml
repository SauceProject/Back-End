
@using ITI.Sauce.ViewModels
@using ITI.Sauce.ViewModels.Shared
@model PaginingViewModel<List<OrderViewModel>>
@{

    Layout = "_Layout";
}

   <!DOCTYPE html>
<html>
<head>
        <link rel="stylesheet" href="~/Content/CSS/Order.css"/>
</head>
<body>

<h2>Order</h2>

<table>
  <tr>
    <th>ID</th>
    <th>NameEN</th>
    <th>NameAR</th>
    <th>Register Date</th>
  </tr>
  <tr>
          @foreach(var o in Model.Data)
{
    <td>@o.ID</td>
    <td>@o.NameEN</td>
    <td>@o.NameAR</td>
    <td>@o.registerDate</td>
}
   

  </tr>
  
</table>

</body>
</html>


