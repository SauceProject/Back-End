@using ITI.Sauce.ViewModels


@model PaginingViewModel<List<IngredientViewModel>>


@{

    Layout = "_Layout";
}

<!DOCTYPE html>
<html>
<head>
    <link rel="stylesheet" href="~/Content/CSS/Ingredient.css" />
</head>
<body>

    <h2>Ingredient</h2>

    <table>
        <tr>
            <th>ID</th>
            <th>NameEN</th>
            <th>NameAR</th>
            <th>ImageUrl</th>
        </tr>
        
            
@foreach (var I in Model.Data)

{
    <tr>
    <td>@I.ID</td>
    <td>@I.NameEN</td>
    <td>@I.NameAR</td>
    <td> <img src="@I.ImageUrl"> </td>
    </tr>
}
    </table>

</body>
</html>

